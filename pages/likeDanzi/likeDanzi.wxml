<view class="head">单字收藏 共{{collectNums}}个</view>

<view class="list" wx:for="{{listData}}">
  <view class="box">
    <navigator class="item" url="/pages/dazi/dazi?id={{item.id}}">
      <view class="box_zi">{{index+1}}</view>
      <view class="box_py">{{item.hanzi}}</view>
      <view class='box_py'>{{item.pinyin}}</view>
    </navigator>
    <image class="more" src="../../image/icon/more.png"></image>
  </view>
</view>




