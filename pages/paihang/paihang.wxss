/* pages/paihang/paihang.wxss */
.bg{
  position: fixed;
  width:100%;
  height:200rpx;
  z-index:10;
  background-image:url(https://www.jiaoyy2020.xyz/paihang.png);
  background-size: 750rpx 200rpx;
}


.head{
  position: relative;
  width: 100%;
  height: 180rpx;
  background-color: #f8a52f;
  position: fixed;
}

.list{
  display: flex;
  flex-direction: column;
}
.box{
  display:flex;
  width:100%;
}
.box_2{
  width:50rpx;
  height:50rpx;
  border:1rpx solid #eeeeee;
  border-radius: 25rpx;
  margin:5rpx 20rpx;
  margin-left: 50rpx;
}
.box_1{
  color:#616161;
}
.body{
  display: flex;
  justify-content: space-between;
  padding: 20rpx 20rpx;
  border-bottom: 1rpx solid #dddddd;
  line-height: 50rpx;
}
.contain{
  position:absolute;
  top:200rpx;
  width: 100%;
}
.mine{
  background-color: gold;
}